    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "Load Quartier Data Script",
            "runtimeExecutable": "npx",
            "runtimeArgs": [
              "tsx",
              "load_quartier_data.ts"
            ],
            "cwd": "${workspaceFolder}/script",
            "skipFiles": ["<node_internals>/**"],
            "sourceMaps": true,
            // "program": "${workspaceFolder}/script/load_insee_data.ts",
        },
        {
            "type": "node",
            "request": "launch",
            "runtimeVersion": "22.12.0",
            "name": "App",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
              "run",
              "dev"
            ],
            "cwd": "${workspaceFolder}",
            "skipFiles": ["<node_internals>/**"],
            "sourceMaps": true,
        }
    ]
}