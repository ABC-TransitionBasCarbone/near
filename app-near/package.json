{
  "name": "app-near",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "prisma": "prisma",
    "test": "jest --passWithNoTests",
    "test:components": "jest --config=jest.components.config.mjs --runInBand",
    "test:shared": "NODE_OPTIONS='--experimental-vm-modules' jest --config=jest.shared.config.mjs --runInBand",
    "test:server": "NODE_OPTIONS='--experimental-vm-modules' jest --config=jest.server.config.mjs --runInBand",
    "check": "next lint && tsc --noEmit",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "seed": "tsx ./scripts/seeds/index.ts",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "load:insee": "/bin/sh ./scripts/load-insee/load_insee_data.sh",
    "load:surveys": "tsx ./scripts/load-insee/load_survey_insee_stats.ts",
    "load:all": "npm run load:insee && npm run load:surveys",
    "dev": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "prepare": "cd .. && husky app-near/.husky"
  },
  "prisma": {
    "seed": "npm run load:insee"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.2",
    "@faker-js/faker": "^9.4.0",
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "^6.3.0",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.50.0",
    "@trpc/client": "^11.0.0-rc.446",
    "@trpc/react-query": "^11.0.0-rc.446",
    "@trpc/server": "^11.0.0-rc.446",
    "bcrypt": "^6.0.0",
    "camelcase-keys": "^9.1.3",
    "dotenv": "^16.4.7",
    "geist": "^1.3.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.0.1",
    "next-auth": "5.0.0-beta.25",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "server-only": "^0.0.1",
    "snakecase-keys": "^8.0.1",
    "superjson": "^2.2.1",
    "tsx": "^4.19.2",
    "weighted": "^1.0.0",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/bcrypt": "^6.0.0",
    "@types/eslint": "^8.56.10",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^20.14.10",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^6.3.0",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "ct3aMetadata": {
    "initVersion": "7.38.1"
  },
  "packageManager": "npm@10.7.0"
}
