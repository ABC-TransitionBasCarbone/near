{
  "eslint.workingDirectories": [
    {"mode": "auto"}
  ],
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.formatOnPaste": true,
  "python.testing.pytestArgs": [
    "api-su"
  ],
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true
}